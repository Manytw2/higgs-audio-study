{
  "name": "higgs-audio-vscode-extension",
  "displayName": "Higgs Audio Chat",
  "description": "AI语音生成助手",
  "version": "1.0.0",
  "publisher": "your-publisher",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:higgsAudio.hello"
  ],
  "main": "./out/extension-minimal.js",
  "contributes": {
    "commands": [
      {
        "command": "higgsAudio.hello",
        "title": "Hello Higgs Audio",
        "category": "Higgs Audio"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
